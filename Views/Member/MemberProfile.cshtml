@model Project14.ViewModels.MemberProfileViewModel

@{
    ViewBag.Title = "個人資料";
    Layout = "~/Views/Shared/_LayoutMember.cshtml";
}

<h2 class="mt-4">👤 個人資料</h2>

<div class="form-group">
    <label>帳號：</label>
    <p class="form-control-plaintext">@Model.UserId</p>
</div>

<div class="form-group">
    <label>姓名：</label>
    <p class="form-control-plaintext">@Model.Name</p>
</div>

<div class="form-group">
    <label>信箱：</label>
    <p class="form-control-plaintext">@Model.Email</p>
</div>